{
    "name": "savaslabs/squabble",
    "description": "Savas Squabble comment server.",
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/lumen-framework": "5.0.*",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/mail": "~5.0",
        "psy/psysh": "@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "behat/behat": "3.1.0",
        "behat/web-api-extension": "~1.0@dev",
        "deminy/behat-rest-testing": "@dev"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "./tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
